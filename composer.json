{
  "name" : "coolms/doctrine-orm",
  "description" : "CoolMS2 Doctrine ORM Module",
  "version" : "0.1.0",
  "type" : "library",
  "keywords" : ["coolms", "doctrine", "orm"],
  "homepage" : "https://github.com/coolms/doctrine-orm",
  "license" : "BSD-3-Clause",
  "authors" : [ {
  	"name" : "Dmitry Popov",
  	"email" : "d.popov@altgraphic.com",
  	"role" : "Developer"
  } ],
  "require" : {
    "php" : ">=5.4.0",
    "doctrine/doctrine-orm" : "2.5.0",
    "doctrine/doctrine-orm-module" : ">=0.9.0",
    "beberlei/DoctrineExtensions" : ">=1.0.0",
    "coolms/doctrine" : "dev-master@dev"
  },
  "require-dev" : {
    "phpunit/phpunit" : "~4.5"
